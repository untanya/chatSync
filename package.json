{
  "name": "chatsync",
  "version": "1.0.0",
  "description": "streaming site with a custom chatbox",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "dev": "node --watch --loader ts-node/esm src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "biome:check": "biome check .",
    "biome:fix": "biome check --write .",
    "mikro:migrate:create": "npx mikro-orm-esm migration:create --config=./src/config/mikro-orm.config.ts",
    "mikro:migrate:up": "npx mikro-orm-esm migration:up --config=./src/config/mikro-orm.config.ts",
    "mikro:migrate:down": "npx mikro-orm-esm migration:down --config=./src/config/mikro-orm.config.ts",
    "mikro:migrate:list": "npx mikro-orm-esm migration:list --config=./src/config/mikro-orm.config.ts",
    "mikro:generate-entities": "npx mikro-orm-esm entity:generate --config=./src/config/mikro-orm.config.ts",
    "mikro:seeder:generate": "npx mikro-orm-esm seeder:generate --config=./src/config/mikro-orm.config.ts",
    "mikro:seeder:run": "npx mikro-orm-esm seeder:run --config=./src/config/mikro-orm.config.ts",
    "mikro:debug": "npx mikro-orm-esm debug --config=./src/config/mikro-orm.config.ts",
    "mikro:refresh": "npx mikro-orm-esm schema:drop --config=./src/config/mikro-orm.config.ts && npx mikro-orm-esm schema:create --config=./src/config/mikro-orm.config.ts && npx mikro-orm-esm migration:up --config=./src/config/mikro-orm.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/untanya/chatSync.git"
  },
  "author": "unTanya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/untanya/chatSync/issues"
  },
  "homepage": "https://github.com/untanya/chatSync#readme",
  "dependencies": {
    "@mikro-orm/core": "^6.4.6",
    "@mikro-orm/entity-generator": "^6.4.6",
    "@mikro-orm/migrations": "^6.4.6",
    "@mikro-orm/mysql": "^6.4.6",
    "@mikro-orm/reflection": "^6.4.6",
    "@mikro-orm/seeder": "^6.4.6",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@types/cors": "^2.8.17",
    "adbkit": "^2.11.1",
    "axios": "^1.7.9",
    "biome": "^0.3.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "glob": "^11.0.1",
    "luxon": "^3.5.0",
    "mysql2": "^3.12.0",
    "puppeteer": "^24.2.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "telegram": "^2.26.22",
    "whatsapp-web.js": "^1.26.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.4.6",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.4",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
